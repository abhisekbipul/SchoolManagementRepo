@inject IHttpContextAccessor HttpContextAccessor
@if(HttpContextAccessor.HttpContext.Session.GetString("SystemAdmin")!= null)
{
    Layout = "SystemAdminLayout";
}else if(HttpContextAccessor.HttpContext.Session.GetString("Administrator")!=null)
{
    Layout = "AdministratorLayout";
}
else if (HttpContextAccessor.HttpContext.Session.GetString("Student") != null)
{
    Layout = "StudentLayout";
}
else
{
    Layout = "_Layout";
}


