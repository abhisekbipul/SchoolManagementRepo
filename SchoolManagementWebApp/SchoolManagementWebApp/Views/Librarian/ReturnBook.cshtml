@model IssuedBook

<partial name="Notification"></partial>
<div class="container-fluid">
    <div class="card">
        <div class="card-body">
            <h5 class="card-title fw-semibold mb-4">Forms</h5>
            <div class="card">
                <div class="card-body">
                    <form method="post" asp-action="ReturnBook">
                        <div class="mb-3">
                            <label class="form-label">BookId</label>
                            <input type="text" class="form-control" asp-for="Bookid">
                        </div>
                        <div class="mb-3">
                            <label class="form-label">Return Date</label>
                            <input type="date" class="form-control" asp-for="ReturnDate">
                        </div>

                        <button type="submit" class="btn btn-primary">Return</button>
                    </form>
                </div>
            </div>

        </div>
    </div>
    <div class="py-6 px-6 text-center">
    </div>
</div>
